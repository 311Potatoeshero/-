//输出成绩等级
#include <stdio.h>
int main()
{
	int a = 0;
	printf("请输入一个百分制成绩：");
	scanf_s("%d", &a);
	char c;
	if (a >= 90)
		c = 'A';
	if (a >= 80 && a <= 89)
		c = 'B';
	if (a >= 70 && a <= 79)
		c = 'C';
	if (a >= 60 && a <= 69)
		c = 'D';
	else
		c = 'E';
	switch (c)
	{
	case 'A':
		printf("成绩是A\n");
		break;
	case 'B':
		printf("成绩是B\n");
		break;
	case 'C':
		printf("成绩是C\n");
		break;
	case 'D':
		printf("成绩是D\n");
		break;
	case 'E':
		printf("不及格");
		break;
	}
	return 0;
}

