#include <stdio.h>
//预处理符号
int main()
{
	printf("%s\n ",__FILE__);
	//文件名+绝对路径
	printf("%d\n ",__LINE__);
	//行号
	printf("%s\n ", __DATE__);
	//日期
	printf("%s\n ", __TIME__);
	//时间
	return 0;
}

